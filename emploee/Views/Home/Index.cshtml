@model IEnumerable<Emploee>


<div class="container" >
    <div class="row" >
        @foreach (var item in Model) {
        <div class="col" >
                <div class="card" style="width: 18rem;">
                    <img src="~/images/emploee/@item.ImageUrl" class="card-img-top" alt="not Found">
                    <div class="card-body">
                        <h5 class="card-title">@item.Name</h5>
                        <p class="card-text">@item.Description</p>
                        <p>@item.Salary</p>
                    </div>
                </div>
        </div>
        }
    </div>
</div>
